@inproceedings{20155001653710,
abstract = {Internet of Things (IoT) enables Smart Cities with novel services. Waste collection in Smart Cities becomes a dynamic process with the proliferation of sensors and actuators embedded on real waste bins. Heterogeneous fleets of trucks are used for efficient waste collection exploiting the diverse road network. In this paper we propose a novel approach by incorporating Low Capacity Trucks (LCTs) and High Capacity Trucks (HCTs). However, HCTs are serving as Mobile Depots (MDs) which are cost efficient and decongest traffic in Smart Cities. A detailed system overview illustrates the architecture of the proposed approach. We also propose novel algorithms which support dynamic waste collection with MDs. Scheduling and routing are transformed to dynamic models. Specifically, we propose a novel scheduling algorithm while we customize an existing routing algorithm. The models where experimentally evaluated with real and synthetic data from the city of St. Petersburg, Russia. The results were promising and proved that the incorporation of MDs is efficient for waste collection in IoT-enabled Smart Cities. Finally, we perform an economic analysis in order to define the economic impact of the proposed solution to the municipality budget for an ownership cost for a period of five years; in which the proposed solution proved to be cost efficient. {\&}copy; Springer International Publishing Switzerland 2015.},
address = {St. Petersburg, Russia},
annote = {Heterogeneous fleet;Internet of Things (IOT);Mobile depots;Novel scheduling algorithms;Scheduling and routing;Sensors and actuators;Smart cities;Waste collection;},
author = {Anagnostopoulos, Theodoros and Zaslavsky, Arkady and Georgiou, Stefanos and Khoruzhnikov, Sergey},
booktitle = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
issn = {03029743},
keywords = {Algorithms;Automobiles;Budget control;Cost benefit},
pages = {80--94},
title = {{High capacity trucks serving as mobile depots for waste collection in iot-enabled smart cities}},
url = {http://dx.doi.org/10.1007/978-3-319-23126-6{\_}8},
volume = {9247},
year = {2015}
}
