@article{Kuran20152942,
abstract = {In this paper, we propose a centralized electric vehicles (EVs) recharge scheduling system for parking lots using a realistic vehicular mobility/parking pattern focusing on individual parking lots. We consider two different types of EV based on their mobility/parking patterns: 1) regular EVs; and 2) irregular EVs. An extensive trace-based vehicular mobility model collected from the Canton of Zurich is used for the regular EVs, and a probabilistic pattern built on top of this trace is used for modeling the behavior of irregular EVs. To the extent of our knowledge, this is the first EV charging scheduling study in the literature that takes into account a realistic vehicular mobility pattern focusing on individual parking lots. We compare the performance of our proposed system with two well-known basic scheduling mechanisms, first come first serve and earliest deadline first, with regard to two objective functions: 1) maximizing the total parking lot revenue; and 2) maximizing the total number of EVs fulfilling their requirements. Comparison results show that our proposed system outperforms well-known basic scheduling mechanisms with regards to both objectives. Parking lots managing the recharging of a high number of EVs will greatly benefit from using such recharge scheduling systems in the context of smart cities. {\textcopyright} 2010-2012 IEEE.},
annote = {cited By 4},
author = {Kuran, M.S.a b and {Carneiro Viana}, A.c and Iannone, L.b d and Kofman, D.b d and Mermoud, G.e and Vasseur, J.P.e},
doi = {10.1109/TSG.2015.2403287},
journal = {IEEE Transactions on Smart Grid},
number = {6},
pages = {2942--2953},
title = {{A smart parking lot management system for scheduling the recharging of electric vehicles}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84960118296{\&}partnerID=40{\&}md5=f357504c487668e436ba8e2aceb76267},
volume = {6},
year = {2015}
}
@article{Yi2016,
abstract = {This paper addresses the problem of optimally placing charging stations in urban areas. Two optimization criteria are used: maximizing the number of reachable households and minimizing overall e-transportation energy cost. The decision making models used for both cases are mixed integer programming with linear and nonlinear energy-aware constraints. A multi-objective optimization model that handles both criteria (number of reachable households and transportation energy) simultaneously is also presented. A number of simulation results are provided for two different cities in order to illustrate the proposed methods. Among other insights, these results show that the multi-objective optimization provides improved placement results.},
author = {Yi, Zonggen and Bauer, Peter H.},
doi = {10.1016/j.tre.2016.04.013},
file = {:home/tarcisio/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yi, Bauer - 2016 - Optimization models for placement of an energy-aware electric vehicle charging infrastructure.pdf:pdf},
issn = {13665545},
journal = {Transportation Research Part E: Logistics and Transportation Review},
keywords = {Charging infrastructure placement,Electric vehicles,Energy-aware optimization models,Multi-objective optimization},
pages = {227--244},
title = {{Optimization models for placement of an energy-aware electric vehicle charging infrastructure}},
volume = {91},
year = {2016}
}
@inproceedings{Bhattacharyya2014,
abstract = {Commuters in modern smart cities need a wide variety of traveller information to be provided to them for taking the right decisions. Drivers need accurate traffic information on urban and suburban roads, parking and electric vehicle charging availability, weather information, dynamic toll rates, fuel prices and so on. Commuters on public transport require bus and train arrival times, traffic-based public transport travel times, public transport breakdown information and so on. Finally, with the growth of inter-modal travel within mega-cities, there is a need to also provide multimodal traveller services that integrate these and other relevant information to provide route options along with travel time estimation, estimated time of arrival and journey plans. Smart cities also have sensors (cameras, light sensors, noise sensors, etc.) deployed through the city to pick up additional data that can be fused to augment regular information and services. Such services and information would be provided over multiple media such as over radio, internet or mobile internet. On the other hand, smart cities can use the massive amounts of data collected within their systems to understand the city's mobility needs and to better plan and react. The data can be used in offline and real-time analyses for short-term and long-term prediction. In this paper, we present a unified traveller information platform for modern smart cities. We discuss the types of data that such a system handles and show how outputs are used within different systems. We discuss interactions with external systems and suggest how the platform can be extended by integrating feedback from client applications to better meet the needs of commuters and cities. Finally, we highlight the key challenges of developing and deploying such a platform and suggest some of the methods to meet these challenges.},
annote = {cited By 0},
author = {Bhattacharyya, S.a and Sindhwani, M.b},
booktitle = {21st World Congress on Intelligent Transport Systems, ITSWC 2014: Reinventing Transportation in Our Connected World},
title = {{A traveller information platform for modern smart cities}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84929190757{\&}partnerID=40{\&}md5=c17e017f0653f82d3c8dabc9c14cf4c5},
year = {2014}
